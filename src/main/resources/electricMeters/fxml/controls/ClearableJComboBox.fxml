<?xml version="1.0" encoding="UTF-8"?>

<?import electricMeters.core.controls.JsonComboBox?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>


<fx:root stylesheets="@../../css/style.css" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <JsonComboBox fx:id="comboBox" prefWidth="150.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" />
      <Button fx:id="clearButton" mnemonicParsing="false" onAction="#clear" style="-fx-padding: 0; -fx-background-color: none;" AnchorPane.rightAnchor="25.0" AnchorPane.topAnchor="5.0">
         <graphic>
            <ImageView fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../icons/clear.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
   </children>
</fx:root>
